<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="xdk.rajyindee.iserv" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>iServPhone</name>
    <description>iServ</description>
    <author>Rajyindee</author>
    <content src="index.html" />
    <splash density="xhdpi" height="960" orientation="portrait" platform="android" src="package-assets/960.png" width="720" />
    <splash density="hdpi" height="640" orientation="portrait" platform="android" src="package-assets/640.png" width="480" />
    <splash density="mdpi" height="470" orientation="portrait" platform="android" src="package-assets/470.png" width="320" />
    <splash density="ldpi" height="426" orientation="portrait" platform="android" src="package-assets/426.png" width="320" />
    <icon density="xhdpi" height="96" platform="android" src="package-assets/96.png" width="96" />
    <icon density="hdpi" height="72" platform="android" src="package-assets/72.png" width="72" />
    <icon density="mdpi" height="48" platform="android" src="package-assets/48.png" width="48" />
    <icon density="ldpi" height="36" platform="android" src="package-assets/36.png" width="36" />
    <config-file mode="merge" parent="/manifest/application" platform="android">
        <activity android:launchMode="singleTop" />
    </config-file>
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="xwalkMultipleApk" value="false" />
</widget>
